    <string name="tutorial_step_1_description">Tortoise is the world’s first savings account for your fitness activities.</string>
    <string name="tutorial_step_2_description">Customize your daily step goal and maximum daily savings amount to start savings seamlessly.</string>
    <string name="tutorial_step_3_description">See live visual updates of your savings and walking steps. Share your accomplishments with friends on Facebook.</string>
    <string name="tutorial_step_4_description">Reach your daily step goal and get an extra savings bonus every day.</string>
    <string name="tutorial_step_5_description">Find opportunities to walk to your appointments and automatically book that time into your mobile calendar.</string>
    <string name="tutorial_step_6_description">No fees <![CDATA[&]]> No minimum balance to maintain unlimited Deposits <![CDATA[&]]> Withdrawals from your account.</string>

    <string name="tutorial_step_6_description_sub_1">Bank-level security of 256-bit encryption.\n(no information is stored on your phone)</string>
    <string name="tutorial_step_6_description_sub_2">Move funds to and from Tortoise without\nincurring any service fees.</string>

